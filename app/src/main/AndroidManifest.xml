<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.rutkovski.checklist">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/title_activity_main2">
            <intent-filter>
            <action android:name="android.intent.action.MAIN" />
            <category android:name="android.intent.category.LAUNCHER" />

            </intent-filter>

        </activity>
        <activity
            android:name=".PointAddNoteActivity"
            android:label="Добавить заметку" />
        <activity android:name=".CheckListTemplateActivity" />
        <activity
            android:name=".CheckListDiagnosticActivity"
            android:label="Чек-лист" />
        <activity
            android:name=".AddCheckListActivity"
            android:label="Добавить чек-лист" />
    </application>

</manifest>